{
    "name": "kqueue/kqueue",
    "description": "A production-grade Node.js-style runtime for Laravel queues",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "queue", "event-loop", "async", "runtime"],
    "authors": [
        {
            "name": "Kunal Ghosh",
            "email": "kunalghosh10000@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/queue": "^10.0|^11.0|^12.0",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "react/event-loop": "^1.5",
        "react/child-process": "^0.6",
        "react/promise": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "KQueue\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "KQueue\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "KQueue\\KQueueServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
